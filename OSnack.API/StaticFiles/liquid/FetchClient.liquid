{% if HasOperations -%}
import { AlertObj, AlertTypes, ErrorDto } from "../../components/Texts/Alert";
import { httpCaller } from "../../_core/appFunc";
import { API_URL, CommonErrors } from "../../_core/constant.Variables";
import { @@Models@@ } from "../../_core/apiModels";
{% for operation in Operations -%}
    {% template Client.Method.Documentation %}
export const use{{ operation.ActualOperationNameUpper }}{{ Class }} = async ({% for parameter in operation.Parameters %}{{ parameter.VariableName }}{% if GenerateOptionalParameters and parameter.IsOptional %}?{% endif %}: {{ parameter.Type }}{{ parameter.TypePostfix }}{% if parameter.IsLast == false %}, {% endif %}{% endfor %}{% if UseAbortSignal %}{% if operation.Parameters.size > 0 %}, {% endif %}signal?: AbortSignal | undefined{% endif %}): Promise<{ data:{% if operation.ResultType =="void" %} null {% else %}{{  operation.ResultType  }} {% endif %}, status: number | undefined}> =>{
        {% template Client.RequestUrl %}
{%     if operation.HasBody -%}
        {% template Client.RequestBody %}
        let response = await httpCaller.{{ operation.HttpMethodUpper | upcase }}(url_, content_);
{%     else -%}
        let response = await httpCaller.{{ operation.HttpMethodUpper | upcase }}(url_);
{%     endif -%}
        if( response?.status === 400){
            await httpCaller.GET(API_URL + "/Authentication/Get/AntiforgeryToken");        
{%     if operation.HasBody -%}
            response = await httpCaller.{{ operation.HttpMethodUpper | upcase }}(url_, content_);
{%     else -%}
            response = await httpCaller.{{ operation.HttpMethodUpper | upcase }}(url_);
{%     endif -%}
        }

        switch(response?.status){
{%  for response in operation.Responses -%}  
                case {{ response.StatusCode }}: 
{%  if response.HasType -%}
{%     if response.IsSuccess -%}
                        var responseData: {{ operation.ResultType }} = await response?.json();
                        return { data: responseData, status: response?.status };
{%      else -%}
                        return response?.json().then((data: ErrorDto[]) => {
                                throw new AlertObj(data, AlertTypes.Error, response?.status);
                        });
{%      endif -%}
{%  elseif response.IsSuccess -%}
                        return { data: null, status: {{ response.StatusCode }} };
{%  else -%}
                        return response?.json().then((data: ErrorDto[]) => {
                                throw new AlertObj([CommonErrors.BadServerResponse], AlertTypes.Error, response?.status);
                        });
{%  endif -%}
{%  endfor %}
                default:
                        CommonErrors.BadServerResponseCode.value = `Server Error Code: ${response?.status || "N/A"}`;
                        throw new AlertObj([CommonErrors.BadServerResponseCode], AlertTypes.Error, response?.status);
        }
  
}
{% endfor -%}
{% endif -%}